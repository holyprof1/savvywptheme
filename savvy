<script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>
<script>
  // NAV color swap
  const nav = document.getElementById('nav');
  addEventListener('scroll',()=>{window.scrollY>100?nav.classList.add('scrolled'):nav.classList.remove('scrolled')});

  // Smooth anchors
  document.querySelectorAll('a[href^="#"]').forEach(a=>{
    a.addEventListener('click',e=>{
      const t=document.querySelector(a.getAttribute('href'));
      if(t){e.preventDefault();t.scrollIntoView({behavior:'smooth',block:'start'})}
    });
  });

  // PRELOADER animations
  const preloader = document.getElementById('preloader');
  const preCount  = document.getElementById('preCount');
  const preRing   = document.getElementById('preRing');
  const preRight  = document.getElementById('preRight');

  gsap.to('.pre-arc',{strokeDashoffset:0,duration:1.1,stagger:.15,ease:'power2.out'});
  gsap.to('.pre-text',{opacity:1,y:0,duration:1,delay:.6,stagger:.2,ease:'power2.out'});
  gsap.to(preRight,{opacity:1,x:0,duration:1,delay:.9,ease:'power2.out'});

  let c=0;
  const intv=setInterval(()=>{
    c=Math.min(c+1,100);
    preCount.textContent=c+'%';
    preRing.style.setProperty('--p',c+'%');
    if(c===100){
      clearInterval(intv);
      setTimeout(()=>{preloader.classList.add('hidden');initAnimations();},250);
    }
  },18);

  function initAnimations(){
    gsap.registerPlugin(ScrollTrigger);

    // Hero words + pills
    gsap.to('.hero-title .word, .hero-title .comma',{y:0,scale:1,rotation:0,opacity:1,duration:1,stagger:0.06,ease:'power4.out',delay:.05});
    gsap.to('.hero-pill',{opacity:1,scale:1,duration:.7,stagger:.1,delay:.6,ease:'back.out(1.7)'});
    gsap.to('.hero-subtitle',{opacity:1,duration:.8,delay:.8});
    gsap.to('.btn-primary, .btn-secondary',{opacity:1,duration:.7,stagger:.12,delay:.9});
    gsap.to('.scroll-indicator',{opacity:1,duration:.8,delay:1});

    // Parallax floaters (subtle)
    gsap.utils.toArray('[data-parallax]').forEach(el=>{
      const amt = +el.getAttribute('data-parallax') || 10;
      gsap.to(el,{y:amt,scrollTrigger:{trigger:'.hero',start:'top top',end:'bottom top',scrub:1}});
    });

    // Grow words on scroll
    gsap.utils.toArray('.grow-text').forEach(el=>{
      gsap.from(el,{scrollTrigger:{trigger:el,start:'top 85%'},scale:.88,opacity:0,duration:.55,ease:'back.out(1.7)'});
    });

    // Section titles wave-in
    document.querySelectorAll('.title-animate').forEach(title=>{
      gsap.to(title.querySelectorAll('.tword'),{
        scrollTrigger:{trigger:title,start:'top 82%'},
        y:0,opacity:1,duration:.55,stagger:.07,ease:'power3.out'
      });
    });

    // DNA cards & featured
    gsap.utils.toArray('.dna-card').forEach((card,i)=>{
      gsap.to(card,{scrollTrigger:{trigger:card,start:'top 90%'},y:0,opacity:1,duration:.65,delay:i*0.05});
    });
    gsap.to('.dna-featured',{scrollTrigger:{trigger:'.dna-featured',start:'top 88%'},y:0,opacity:1,duration:.8});

    // Services cascade + pointer glow follows mouse
    gsap.utils.toArray('.service-card').forEach((card,i)=>{
      gsap.to(card,{scrollTrigger:{trigger:card,start:'top 90%'},y:0,opacity:1,duration:.6,delay:i*0.04});
      card.addEventListener('mousemove',e=>{
        const r=card.getBoundingClientRect();
        card.style.setProperty('--x', (e.clientX-r.left)+'px');
        card.style.setProperty('--y', (e.clientY-r.top)+'px');
      });
    });
  }

  // Scroll-to-top progress
  const scrollBtn=document.getElementById('scrollTopBtn');
  const scrollPct=document.getElementById('scrollPct');
  function setScrollProgress(){
    const st=window.scrollY;
    const h=document.documentElement.scrollHeight - innerHeight;
    const p=h>0?Math.round((st/h)*100):0;
    document.documentElement.style.setProperty('--scroll',p);
    scrollBtn.style.setProperty('--scroll',p);
    scrollPct.textContent=p+'%';
    st>380?scrollBtn.classList.add('visible'):scrollBtn.classList.remove('visible');
  }
  setScrollProgress();
  addEventListener('scroll',setScrollProgress,{passive:true});
  scrollBtn.addEventListener('click',()=>scrollTo({top:0,behavior:'smooth'}));

  // Swiper
  const caseSwiper=new Swiper('.caseSwiper',{
    slidesPerView:1,spaceBetween:26,loop:true,autoplay:{delay:5000},
    pagination:{el:'.swiper-pagination',clickable:true},
    breakpoints:{768:{slidesPerView:2},1024:{slidesPerView:3}}
  });
</script>
</body>
</html>